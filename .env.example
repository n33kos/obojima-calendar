# GitHub Gist Configuration
# Copy this file to .env and fill in your values

# Your GitHub username
VITE_GIST_USERNAME=your-github-username

# Your Gist ID (found in the URL: https://gist.github.com/username/GIST_ID)
VITE_GIST_ID=your-gist-id

# The filename of your JSON file in the Gist
VITE_GIST_FILENAME=obojima-party-tracker.json

# Optional: Auto-refresh interval in milliseconds (default: 60000 = 1 minute)
VITE_GIST_REFRESH_INTERVAL=60000
